{
  "name": "Testing & Quality Agent",
  "type": "agent",
  "framework": "openapi-ai-agents-standard",
  "features": [
    "unit-tests",
    "integration-tests",
    "ci-cd-pipeline",
    "quality-metrics"
  ],
  "testStrategy": "behavior-driven",
  "coverage": {
    "target": 80,
    "enforced": true
  },
  "quality": {
    "linting": true,
    "typeChecking": true,
    "securityScanning": true
  },
  "deployment": {
    "port": 3006,
    "healthCheck": "/health",
    "metricsEndpoint": "/metrics"
  }
}