{
  "name": "Compliance & Governance Agent",
  "type": "agent",
  "framework": "openapi-ai-agents-standard",
  "features": [
    "iso-42001-validation",
    "nist-ai-rmf",
    "eu-ai-act",
    "certification-system"
  ],
  "testStrategy": "behavior-driven",
  "coverage": {
    "target": 80,
    "enforced": true
  },
  "quality": {
    "linting": true,
    "typeChecking": true,
    "securityScanning": true
  },
  "deployment": {
    "port": 3004,
    "healthCheck": "/health",
    "metricsEndpoint": "/metrics"
  }
}